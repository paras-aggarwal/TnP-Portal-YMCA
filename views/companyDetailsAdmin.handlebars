<div class="conteudo-box">
    <div class="conteudo-boxInner">
      <table id="company_details" class="display" width="100%" cellspacing="0">
        <tr>
          <div class="">
            <td><span style="font-size:1.5em;font-weight:bold;">Event Name: </span>
            <td><span style="font-size:1.5em;">{{result.name}}</span>
          </div>
        </tr>
        <tr>
          <div class="">
            <td><span style="font-size:1.5em;font-weight:bold;">Position: </span>
            <td><span style="font-size:1.5em;">{{result.position}}</span>
          </div>
        </tr>
        <tr>
          <div class="">
            <td><span style="font-size:1.5em;font-weight:bold;">10th Percentage: </span>
            <td><span style="font-size:1.5em;">{{result.criteria.percent_10}}</span>
          </div>
        </tr>
        <tr>
          <div class="">
            <td><span style="font-size:1.5em;font-weight:bold;">12th Percentage: </span>
            <td><span style="font-size:1.5em;">{{result.criteria.percent_10}}</span>
          </div>
        </tr>
        <tr>
          <div class="">
            <td><span style="font-size:1.5em;font-weight:bold;">Diploma Percentage: </span>
            <td><span style="font-size:1.5em;">{{result.criteria.percent_diploma}}</span>
          </div>
        </tr>
        <tr>
          <div class="">
            <td><span style="font-size:1.5em;font-weight:bold;">CGPA: </span>
            <td><span style="font-size:1.5em;">{{result.cpi}}</span>
          </div>
        </tr>
        <tr>
          <div class="">
            <td><span style="font-size:1.5em;font-weight:bold;">Backlogs: </span>
            <td><span style="font-size:1.5em;">{{result.criteria.backlogs}}</span>
          </div>
        </tr>
        <tr>
          <div class="">
            <td><span style="font-size:1.5em;font-weight:bold;">Branch: </span>
            <td><span style="font-size:1.5em;">{{result.criteria.branch}}</span>
          </div>
        </tr>
        <tr>
          <div class="">
            <td><span style="font-size:1.5em;font-weight:bold;">Schedule: </span>
